<h1>Compatible Receivers for <%= model %> (<%= type %>, <%= country %>)</h1>
<table border="1">
  <tr>
    <th>ID</th>
    <th>Country</th>
    <th>Type</th>
    <th>Model</th>
  </tr>
  <% receivers.forEach(r => { %>
    <tr>
      <td><%= r.id %></td>
      <td><%= r.country %></td>
      <td><%= r.type %></td>
      <td><%= r.model %></td>
    </tr>
  <% }) %>
</table>
